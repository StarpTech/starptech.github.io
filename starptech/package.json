{
  "name": "starptech",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run collect && next dev",
    "dev": "npm run collect && next dev",
    "now-build": "npm run collect && next build",
    "post": "node scripts/create.js",
    "collect": "node scripts/collect-meta.js",
    "export": "npm run now-build && next export",
    "deploy": "touch out/.nojekyll && touch out/CNAME && echo \"starptech.de\" >> out/CNAME && gh-pages -d out -b master -t"
  },
  "dependencies": {
    "@zeit-ui/react": "^1.5.0",
    "@zeit-ui/react-icons": "^1.2.2",
    "@zeit-ui/react-prism": "^1.0.1",
    "ms": "^2.1.2",
    "next": "^9.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/loader": "^1.6.4",
    "@next/mdx": "^9.4.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "chalk": "^4.0.0",
    "extract-mdx-metadata": "^1.0.0",
    "fs-extra": "^9.0.0",
    "gh-pages": "^2.2.0",
    "prompt-sync": "^4.2.0",
    "rehype-join-line": "^1.0.2",
    "styled-jsx": "^3.3.0"
  }
}
